{
  "name": "tourze/symfony-schedule-entity-clean-bundle",
  "description": "Symfony bundle for automatic scheduled cleanup of Doctrine entities based on creation time and cron expressions",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "dragonmantank/cron-expression": "^3.4",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/async-contracts": "0.0.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/symfony-cron-job-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ScheduleEntityCleanBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ScheduleEntityCleanBundle\\Tests\\": "tests"
    }
  }
}